<Page x:Class="HighLand.RightPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HighLand"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="400"
      Title="RightPage"
      Background="Beige">

    <Grid>
        <Button x:Name="btnUser" Content="User Management" HorizontalAlignment="Center" Margin="0,27,0,0" VerticalAlignment="Top" Height="63" Width="380" Background="#FFA24B3D"/>
        <Button x:Name="btnProduct" Content="Product Management" HorizontalAlignment="Center" Margin="0,107,0,0" VerticalAlignment="Top" Height="63" Width="380" Background="#FFA24B3D" Click="btnProduct_Click"/>
        <Button x:Name="btnExist" Content="Exist" HorizontalAlignment="Center" Margin="0,187,0,0" VerticalAlignment="Top" Height="63" Width="380" Background="#FFA24B3D" Click="btnExist_Click"/>

    </Grid>
</Page>
